
config wifi-device 'radio0'
 	option type 'mac80211'
 	option channel 'auto'
 	option hwmode '11g'
 	option path 'platform/ar933x_wmac'
 	option htmode 'HT20'
 	option disabled '0'
 	option txpower '30'
 	option country 'US'
 
